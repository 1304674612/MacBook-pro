{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C: Debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/c_project/app",
      "cwd": "${workspaceFolder}/c_project",
      "MIMode": "lldb",
      "preLaunchTask": "C: Build"
    },
    {
      "name": "Java: Launch",
      "type": "java",
      "request": "launch",
      "mainClass": "com.example.Main",
      "projectName": "java_project"
    },
    {
      "name": "Python: Launch",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/python_project/main.py",
      "console": "integratedTerminal"
    }
  ]
}
